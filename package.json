{
  "name": "@zhangziheng/frame-sense",
  "version": "0.1",
  "description": "一个图像识别模型的命令行工具，用于为视频和图片文件生成语义化文件名。",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "frame-sense": "./dist/cli.js",
    "fren": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsup src/cli.ts --format esm --target node18",
    "start": "node dist/cli.js",
    "lint": "biome check . & tsc --noEmit",
    "lint:fix": "biome check --fix . || tsx ",
    "format": "biome format --write .",
    "fl": "pnpm format && pnpm lint",
    "type-check": "tsc --noEmit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "pub": "npm publish --access public --no-git-checks"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "video",
    "ai",
    "rename",
    "frame",
    "ffmpeg",
    "video-processing",
    "image-processing",
    "file-renaming",
    "ai-powered",
    "automated-naming",
    "typescript",
    "nodejs",
    "cli-tool",
    "google-gemini",
    "media-management",
    "video-analysis",
    "content-organization",
    "batch-processing",
    "command-line",
    "automation-tool",
    "developer-tools"
  ],
  "author": {
    "name": "zhangziheng",
    "email": "shadow1746556951@gmail.com",
    "url": "https://github.com/Shadowzzh"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shadowzzh/frame-sense.git"
  },
  "bugs": {
    "url": "https://github.com/Shadowzzh/frame-sense/issues"
  },
  "homepage": "https://github.com/Shadowzzh/frame-sense#readme",
  "dependencies": {
    "@google/genai": "^1.8.0",
    "@types/inquirer": "^9.0.8",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "commander": "^14.0.0",
    "conf": "^14.0.0",
    "inquirer": "^12.7.0",
    "ora": "^8.2.0",
    "sharp": "^0.34.2",
    "undici": "^7.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.10.1",
    "@types/sharp": "^0.31.1",
    "conventional-changelog-cli": "^5.0.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
