{
  "name": "frame-sense",
  "version": "0.0.1",
  "description": "一个基于 AI 的智能视频重命名 CLI 命令行工具",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "frame-sense": "./dist/cli.js",
    "fren": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsup src/cli.ts --format esm --dts --clean",
    "start": "node dist/cli.js",
    "lint": "biome check . && tsc --noEmit",
    "lint:fix": "biome check --fix .",
    "format": "biome format --write .",
    "type-check": "tsc --noEmit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "keywords": [
    "cli",
    "video",
    "ai",
    "rename",
    "frame",
    "ffmpeg"
  ],
  "author": {
    "name": "zhangziheng",
    "email": "shadow1746556951@gmail.com",
    "url": "https://github.com/Shadowzzh"
  },
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.8.0",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.0",
    "conf": "^14.0.0",
    "figlet": "^1.7.0",
    "ora": "^8.1.1",
    "sharp": "^0.34.2",
    "undici": "^7.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@types/cli-progress": "^3.11.6",
    "@types/figlet": "^1.5.8",
    "@types/node": "^22.10.1",
    "@types/sharp": "^0.31.1",
    "conventional-changelog-cli": "^5.0.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
